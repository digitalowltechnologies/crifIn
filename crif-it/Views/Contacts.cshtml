@using Crif.It.Models;
@using Umbraco.Cms.Web.Common.PublishedModels;
@using Crif.It.Utils;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<Contacts>

@{
    Layout = "Master.cshtml";
}

<section class="textonly-heading pb-lg-3">
    <div class="container-lg">
        <div class="row">
            <div class="col-12 banner">
                <div class="caption">
                    @await(Html.PartialAsync("Breadcrumb", Model))
                    <h1 class="h1 fade-in-box">@Model.PageTitle</h1>
                    <p class="big-text fade-in-box">@Model.PageSubtitle</p>
                </div>
            </div>
        </div>
    </div>
</section>

@Html.GetBlockListHtml(Model.BlockList)