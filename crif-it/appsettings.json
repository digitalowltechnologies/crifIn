{
  "$schema": "./appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "ProxyConfig": {
    "ServerAddress": "",
    "ServerPort": "",
    "ServerUserName": "",
    "ServerPassword": ""
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "9722400f-b931-40fc-a2c8-2e5f2d508b72",
        "SanitizeTinyMce": true,
        "UmbracoCssPath": "~/assets/css",
        "UmbracoScriptsPath": "~/assets/script",
      },
      "Content": {
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeManual",
        "ModelsNamespace": "Crif.It.Models",
        "ModelsDirectory": "~/Models"
      },
      "RuntimeMinification": {
        "UseInMemoryCache": true,
        "CacheBuster": "Version",
        "Version": "1234"
      }
    },
    "Forms": {
      "FieldTypes": {
        "Recaptcha2": {
          "PrivateKey": "6Ld_q3AiAAAAAMPNzkGQF5gve98Yxm75uLNN3w3x",
          "PublicKey": "6Ld_q3AiAAAAAFk3yyHe62MtxqxCccwLVqwncI3G"
        }
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=|DataDirectory|/Umbraco.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.Sqlite"
  },
  "uSync": {
    "Settings": {
      "RootFolder": "uSync/v10",
      "DefaultSet": "Default",
      "ImportAtStartup": "None",
      "ExportAtStartup": "None",
      "ExportOnSave": "None",
      "UiEnabledGroups": "All",
      "ReportDebug": false,
      "AddOnPing": true,
      "RebuildCacheOnCompletion": false,
      "FailOnMissingParent": false,
      "CacheFolderKeys": true,
      "ShowVersionCheckWarning": true,
      "CustomMappings": {},
      "SignalRRoot": "",
      "EnableHistory": true,
      "DefaultExtension": "config",
      "ImportOnFirstBoot": false,
      "FirstBootGroup": "All"
    },
    "Sets": {
      "Default": {
        "Enabled": true,
        "HandlerGroups": [],
        "DisabledHandlers": [
          "PublishedContentHandler",
          "MediaFileHandler",
          "PartialFileHandler",
          "PartialMacroFileHandler",
          "ScriptFileHandler",
          "StyleSheetHandler"
        ],
        "HandlerDefaults": {
          "Enabled": true,
          "Actions": [],
          "UseFlatStructure": true,
          "GuidNames": false,
          "FailOnMissingParent": false,
          "Group": "",
          "Settings": {}
        },
        "Handlers": {},
        "IsSelectable": false
      }
    }
  }
}
